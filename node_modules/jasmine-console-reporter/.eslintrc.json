{
    "parserOptions": {
    },
    "env": {
        "node": true,
        "jasmine": true
    },
    "plugins": [

    ],
    "globals": {
    },
    "extends": "xo",
    // "extends": "eslint:recommended",
    "rules": {
        "indent": [2, 4, {"SwitchCase": 1}],
        "curly": [2, "multi-line", "consistent"],
        "quotes": ["error", "single"],
        "one-var": 0,
        "no-unused-vars": 1,
        "object-curly-spacing": 0,
        "operator-linebreak": 0,
        "padded-blocks": 0,
        "no-negated-condition": 0,
        "quote-props": 0,
        "capitalized-comments": 0,
        "no-multi-spaces": 0,
        "no-prototype-builtins": 0
    }
}
